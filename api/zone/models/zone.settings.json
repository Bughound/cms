{
  "kind": "collectionType",
  "collectionName": "zones",
  "info": {
    "name": "zone"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "geojson": {
      "type": "json"
    },
    "name": {
      "type": "string"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user"
    },
    "notifications": {
      "type": "boolean"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "economic",
        "sanitary"
      ]
    },
    "plants": {
      "collection": "plant"
    }
  }
}
