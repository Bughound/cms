{
  "kind": "collectionType",
  "collectionName": "observation",
  "info": {
    "name": "Observation"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "geojson": {
      "type": "json"
    },
    "date": {
      "type": "datetime"
    },
    "place": {
      "type": "string"
    },
    "count": {
      "type": "integer",
      "default": 1
    },
    "wheater": {
      "type": "json"
    },
    "taxon": {
      "model": "taxon"
    }
  }
}
